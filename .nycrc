{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "recursive": true,
    "compact": false,
    "useInlineSourceMaps": false,
    "extension": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx"
    ],
    "exclude": [
        "**/*.config.*"
    ],
    "parser-plugins": [
        "typescript"
    ]
}
